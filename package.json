    "inversify": "^6.0.1",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.2.1",
    "koa2-swagger-ui": "^5.6.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^4.1.0",
