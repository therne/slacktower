{
  "name": "slacktower",
  "version": "1.0.0",
  "description": "",
  "packageManager": "pnpm@7.9.4",
  "main": "index.js",
  "scripts": {
    "build": "tsoa spec-and-routes && mkdir -p ./build && cp -r ./generated/ ./build/generated && tsc",
    "start": "ts-node -r tsconfig-paths/register src/app.ts",
    "generate": "tsoa spec-and-routes",
    "lint": "eslint ./src/**/*.ts && prettier -c ./src/**/*.ts",
    "dev": "concurrently -n app,generate nodemon \"nodemon -x pnpm generate\"",
    "swagger": "tsoa spec && swagger-ui --port=8181 ./generated/openapi/swagger.json",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
